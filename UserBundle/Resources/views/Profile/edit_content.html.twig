{% form_theme form 'FOSUserBundle::form.html.twig' %}
<form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(form) }} method="POST" class="fos_user_profile_edit {{ formclass }}">
    {{ form_widget(form) }}

    <div class="control-group">
        <label class="control-label">&nbsp;</label>
        <div class="controls">
            <input type="submit" class="btn" value="{{ 'profile.edit.submit'|trans({}, 'AppUserBundle') }}" />
        </div>
    </div>
    

</form>
